{# _form.html.twig meeting form view #}

{% form_theme form 'OrganizationManagementBundle:Form:_checkboxes.html.twig' %}

{{ form_start(form) }}
{{ form_errors(form) }}

<div class="form-group">
    {{ form_row(form.name, {'attr': { 'class': 'form-control' } } ) }}
</div>

<div class="form-group">
    {{ form_row(form.startDate, {'attr': { 'class': 'calendar' } } ) }}
</div>

<div class="form-group">
    {{ form_row(form.city, {'attr': { 'class': 'form-control' } } ) }}
</div>

<div class="form-group">
    {{ form_row(form.topics) }}
</div>

<div class="form-group">
    {{ form_row(form.partners) }}
</div>

<div class="form-group">
    {{ form_row(form.sponsors) }}
</div>

<div class="form-group">
    {{ form_row(form.description, {'attr': { 'class': 'form-control' } } ) }}
</div>

<div class="form-group">
    {{ form_row(form.ticketsUrl, {'attr': { 'class': 'form-control' } } ) }}
</div>

<div class="form-group">
    {{ form_row(form.facebookUrl, {'attr': { 'class': 'form-control' } } ) }}
</div>

<div class="form-group">
    {{ form_row(form.submit, {'attr': { 'class': 'btn btn-default' } } ) }}
</div>

{{ form_end(form) }}